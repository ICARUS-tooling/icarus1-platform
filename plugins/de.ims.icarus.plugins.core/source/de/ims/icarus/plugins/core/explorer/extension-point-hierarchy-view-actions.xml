<?xml version="1.0" ?><!DOCTYPE actions SYSTEM "action-list.dtd"><actions>	<!--		EXTENSION POINT HIERARCHY VIEW	-->			<!-- Show Hierarchy -->	<action id="plugins.core.extensionPointHierarchyView.showHierarchyAction"		name="${id}.name" desc="${id}.description" icon="hierarchy_co.gif" type="toggle" />			<!-- Show Super-Hierarchy -->	<action id="plugins.core.extensionPointHierarchyView.showSuperHierarchyAction"		name="${id}.name" desc="${id}.description" icon="super_co.gif" type="toggle" />			<!-- Show Sub-Hierarchy -->	<action id="plugins.core.extensionPointHierarchyView.showSubHierarchyAction"		name="${id}.name" desc="${id}.description" icon="sub_co.gif" type="toggle" />			<!-- Hierarchy Type Set -->	<action-set id="plugins.core.extensionPointHierarchyView.hierarchyTypeSet">		<group id="plugins.core.extensionPointHierarchyView.hierarchyTypeGroup">			<action idref="plugins.core.extensionPointHierarchyView.showHierarchyAction" />			<action idref="plugins.core.extensionPointHierarchyView.showSuperHierarchyAction" />			<action idref="plugins.core.extensionPointHierarchyView.showSubHierarchyAction" />		</group>	</action-set>			<!-- Show Parameters in Hierarchy -->	<action id="plugins.core.extensionPointHierarchyView.lockOutlineAction"		name="${id}.name" desc="${id}.description" icon="impl_co.gif" type="toggle"/>			<!-- Show All Inherited Parameters -->	<action id="plugins.core.extensionPointHierarchyView.showInheritedParametersAction"		name="${id}.name" desc="${id}.description" icon="inher_co.gif" type="toggle"/>			<!-- Sort Parameters by Extension-Point -->	<action id="plugins.core.extensionPointHierarchyView.sortParametersByExtensionPointAction"		name="${id}.name" desc="${id}.description" icon="definingtype_sort_co.gif" type="toggle"/>		<!-- Selection List for header area -->	<action-list id="plugins.core.extensionPointHierarchyView.typeSelectionList">		<item type="glue" />		<item type="action-set" value="plugins.core.extensionPointHierarchyView.hierarchyTypeSet" />
	</action-list>		<!-- Parameter Outline List -->	<action-list id="plugins.core.extensionPointHierarchyView.parameterOutlineList">		<item type="custom" value="label"/>		<item type="glue" />		<item type="action" value="plugins.core.extensionPointHierarchyView.lockOutlineAction" />		<separator />		<item type="action" value="plugins.core.extensionPointHierarchyView.showInheritedParametersAction" />		<item type="action" value="plugins.core.extensionPointHierarchyView.sortParametersByExtensionPointAction" />
	</action-list>				<!-- Show Extension Point -->	<action id="plugins.core.extensionPointHierarchyView.showExtensionPointAction"		name="${id}.name" desc="${id}.description" icon="plugin_obj.gif" />		<!-- Show Declaring Plug-In -->	<action id="plugins.core.extensionPointHierarchyView.showPluginAction"		name="${id}.name" desc="${id}.description" icon="ext_point_obj.gif" />		<!-- Popup Menu List -->	<action-list id="plugins.core.extensionPointHierarchyView.popupMenuList">		<item type="action" value="plugins.core.extensionPointHierarchyView.showExtensionPointAction" />		<item type="action" value="plugins.core.extensionPointHierarchyView.showPluginAction" />	</action-list></actions>