<?xml version="1.0" ?>
<!DOCTYPE actions SYSTEM "action-list.dtd">
<actions>

	<!--
		WEBLICHT WEBSERVICE VIEW
	-->
	
	<!-- Save Webservice -->
	<action id="plugins.weblicht.weblichtWebserviceView.saveWebserviceAction"
		name="${id}.name" desc="${id}.description" icon="save_webservice.png" />
		
	<!-- New Webservice -->
	<action id="plugins.weblicht.weblichtWebserviceView.newWebserviceAction"
		name="${id}.name" desc="${id}.description" icon="add_webservice.png" />
		
	<!-- Delete Webservice -->
	<action id="plugins.weblicht.weblichtWebserviceView.deleteWebserviceAction"
		name="${id}.name" desc="${id}.description" icon="delete_webservice.png" />
	
	<!-- Clone Webchain -->
	<action id="plugins.weblicht.weblichtWebserviceView.cloneWebserviceAction"
		name="${id}.name" desc="${id}.description" icon="clone_webservice.png" />
		
	<!-- Edit Webservice -->
	<action id="plugins.weblicht.weblichtWebserviceView.editWebserviceAction"
		name="${id}.name" desc="${id}.description" icon="edit_webservice.png" />
	
	<!-- Sort ascending Webservice -->
	<action id="plugins.weblicht.weblichtWebserviceView.ascWebserviceSortAction"
		name="${id}.name" desc="${id}.description" icon="sort_rows_asc_alph.gif" />
	
	<!-- Sort descending Webservice -->
	<action id="plugins.weblicht.weblichtWebserviceView.descWebserviceSortAction"
		name="${id}.name" desc="${id}.description" icon="sort_rows_desc_alph.gif" />
	
	<!-- ToolBar List for header area -->
	<action-list id="plugins.weblicht.weblichtWebserviceView.toolBarList">
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.saveWebserviceAction" />
		<separator />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.newWebserviceAction" />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.deleteWebserviceAction" />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.cloneWebserviceAction" />
		<separator />		
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.editWebserviceAction" />
		<separator />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.ascWebserviceSortAction" />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.descWebserviceSortAction" />
	</action-list>

		
	<!-- Popup Menu List -->
	<action-list id="plugins.weblicht.weblichtWebserviceView.popupMenuList">
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.newWebserviceAction" />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.deleteWebserviceAction" />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.cloneWebserviceAction" />
		<separator />
		<item type="action" value="plugins.weblicht.weblichtWebserviceView.editWebserviceAction" />
	</action-list>
	
</actions>