<?xml version="1.0" ?>
<!DOCTYPE actions SYSTEM "action-list.dtd">
<actions>

	<!--
		PAINTE EDITOR VIEW
	-->
	
		
	<!-- GENERAL ACTIONS --> 
		
	<!-- Open Preferences -->
	<action id="plugins.prosody.painteEditorView.openPreferencesAction"
		template="actions.default.openPreferencesAction" />
		
	<!-- Remove PaIntE Params -->
	<action id="plugins.prosody.painteEditorView.removePainteParamsAction"
		name="${id}.name" desc="${id}.description" icon="chart_curve_delete.png" accel="delete" />
		
	<!-- Edit PaIntE Params -->
	<action id="plugins.prosody.painteEditorView.usePainteParamsAction"
		name="${id}.name" desc="${id}.description" icon="chart_curve_edit.png" accel="enter" />
		
	<!-- Rename PaIntE Params -->
	<action id="plugins.prosody.painteEditorView.renamePainteParamsAction"
		template="actions.default.renameAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Export Params Registry -->
	<action id="plugins.prosody.painteEditorView.exportParamsRegistryAction"
		template="actions.default.exportAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Import Params Registry -->
	<action id="plugins.prosody.painteEditorView.importParamsRegistryAction"
		template="actions.default.importAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Clear Params Registry -->
	<action id="plugins.prosody.painteEditorView.clearParamsRegistryAction"
		template="actions.default.clearAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Collapse All Panels -->
	<action id="plugins.prosody.painteEditorView.collapseAllPanelsAction"
		template="actions.default.collapseAllAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Expand All Panels -->
	<action id="plugins.prosody.painteEditorView.expandAllPanelsAction"
		template="actions.default.expandAllAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Copy Params -->
	<action id="plugins.prosody.painteEditorView.copyPainteParamsAction"
		template="actions.default.copyAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Copy Params Id -->
	<action id="plugins.prosody.painteEditorView.copyPainteParamsIdAction"
		icon="cpyqual_menu.gif"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Paste Params -->
	<action id="plugins.prosody.painteEditorView.pastePainteParamsAction"
		template="actions.default.pasteAction"
		name="${id}.name" desc="${id}.description" />
	
			
	<!-- PANEL ACTIONS -->
		
	<!-- Add Panel -->
	<action id="plugins.prosody.painteEditorView.addParamsPanelAction"
		template="actions.default.addAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Remove Panel -->
	<action id="plugins.prosody.painteEditorView.removeParamsPanelAction"
		template="actions.default.removeAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Color Panel -->
	<action id="plugins.prosody.painteEditorView.colorParamsPanelAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Import Panel -->
	<action id="plugins.prosody.painteEditorView.importParamsPanelAction"
		template="actions.default.pasteAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Export Panel -->
	<action id="plugins.prosody.painteEditorView.exportParamsPanelAction"
		template="actions.default.copyAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Export Param Id -->
	<action id="plugins.prosody.painteEditorView.exportParamsPanelIdAction"
		icon="cpyqual_menu.gif"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Rename Panel -->
	<action id="plugins.prosody.painteEditorView.renameParamsPanelAction"
		template="actions.default.renameAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Clear Panel -->
	<action id="plugins.prosody.painteEditorView.clearParamsPanelAction"
		template="actions.default.clearAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Save Panel -->
	<action id="plugins.prosody.painteEditorView.saveParamsPanelAction"
		template="actions.default.saveAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Save Panel As -->
	<action id="plugins.prosody.painteEditorView.saveParamsPanelAsAction"
		template="actions.default.saveAsAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Undo Panel -->
	<action id="plugins.prosody.painteEditorView.undoParamsPanelAction"
		template="actions.default.undoAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Redo Panel -->
	<action id="plugins.prosody.painteEditorView.redoParamsPanelAction"
		template="actions.default.redoAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- ToolBar List for header area -->
	<action-list id="plugins.prosody.painteEditorView.toolBarList">
		<item type="action" value="plugins.prosody.painteEditorView.openPreferencesAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.removePainteParamsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.copyPainteParamsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.copyPainteParamsIdAction" />
		<item type="action" value="plugins.prosody.painteEditorView.usePainteParamsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.renamePainteParamsAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.clearParamsRegistryAction" />
		<item type="action" value="plugins.prosody.painteEditorView.importParamsRegistryAction" />
		<item type="action" value="plugins.prosody.painteEditorView.exportParamsRegistryAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.pastePainteParamsAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.collapseAllPanelsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.expandAllPanelsAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.addParamsPanelAction" />
		<item type="glue" />
	</action-list>
		
	<!-- Popup-Menu list -->
	<action-list id="plugins.prosody.painteEditorView.popupMenuList">
		<item type="action" value="plugins.prosody.painteEditorView.removePainteParamsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.copyPainteParamsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.copyPainteParamsIdAction" />
		<item type="action" value="plugins.prosody.painteEditorView.usePainteParamsAction" />
		<item type="action" value="plugins.prosody.painteEditorView.renamePainteParamsAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.clearParamsRegistryAction" />
		<item type="action" value="plugins.prosody.painteEditorView.importParamsRegistryAction" />
		<item type="action" value="plugins.prosody.painteEditorView.exportParamsRegistryAction" />
	</action-list>
		
	<!-- Parameter-Panel Menu list -->
	<action-list id="plugins.prosody.painteEditorView.parameterPanelMenuList">
		<item type="action" value="plugins.prosody.painteEditorView.addParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.removeParamsPanelAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.renameParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.clearParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.colorParamsPanelAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.importParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.exportParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.exportParamsPanelIdAction" />
		<item type="action" value="plugins.prosody.painteEditorView.saveParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.saveParamsPanelAsAction" />
		<separator />
		<item type="action" value="plugins.prosody.painteEditorView.undoParamsPanelAction" />
		<item type="action" value="plugins.prosody.painteEditorView.redoParamsPanelAction" />
	</action-list>
	
</actions>