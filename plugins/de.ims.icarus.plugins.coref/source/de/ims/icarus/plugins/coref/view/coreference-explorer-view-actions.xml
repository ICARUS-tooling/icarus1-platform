<?xml version="1.0" ?>
<!DOCTYPE actions SYSTEM "action-list.dtd">
<actions>

	<!--
		COREFERENCE EXPLORER VIEW
	-->
		
	<!-- Inspect Document -->
	<action id="plugins.coref.coreferenceExplorerView.inspectDocumentAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Analyse Document -->
	<action id="plugins.coref.coreferenceExplorerView.analyzeDocumentAction"
		name="${id}.name" desc="${id}.description" />
		
	<!-- Popup-Menu list -->
	<action-list id="plugins.coref.coreferenceExplorerView.popupMenuList">
		<item type="action" value="plugins.coref.coreferenceExplorerView.inspectDocumentAction" />
		<item type="action" value="plugins.coref.coreferenceExplorerView.analyzeDocumentAction" />
	</action-list>
	
</actions>